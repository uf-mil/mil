<?xml version="1.0"?>
<!-- Sets robot_description parameter to Navigator's URDF -->
<launch>
  <arg name="simulate_cams" default="true" />
  <arg name="sub" default="sub8"/>
  <param if="$(arg simulate_cams)" name="robot_description" textfile="$(find sub8_gazebo)/urdf/$(arg sub).urdf"/>
  <param unless="$(arg simulate_cams)" name="robot_description" textfile="$(find sub8_gazebo)/urdf/$(arg sub)_no_cams.urdf"/>

  <rosparam param="robot_parameters">
    volume: 0.04822
    drag:
      linear_coeffs: [250, 250, 250]
      angular_coeffs: [100, 100, 100]
    height: 0.59
    fluid_density: 997.0
    air_density: 1.225
    G: 9.81
  </rosparam>
</launch>
