<?xml version="1.0"?>
<package format="2">
  <name>subjugator_gazebo</name>
  <version>1.1.0</version>
  <description>The subjugator_gazebo package</description>
  <maintainer email="matthew.langford95@gmail.com">Matthew</maintainer>
  <maintainer email="guojiyao@todo.todo">Jiyao</maintainer>
  <maintainer email="jpanikulam@ufl.edu">Jake</maintainer>
  <depend>gazebo_ros</depend>
  <depend>geometry_msgs</depend>
  <depend>message_generation</depend>
  <depend>mil_gazebo</depend>
  <depend>roscpp</depend>
  <depend>rospy</depend>
  <depend>std_msgs</depend>
  <depend>subjugator_msgs</depend>
  <depend>xacro</depend>
  <build_export_depend>message_runtime</build_export_depend>
  <exec_depend>message_runtime</exec_depend>
  <license>MIT</license>
  <buildtool_depend>catkin</buildtool_depend>
  <export>
    <!-- The secret sauce, this allows you to add custom textures and models into gazebo-->
    <gazebo_ros gazebo_media_path="${prefix}" gazebo_plugin_path="${prefix}/lib" gazebo_model_path="${prefix}/models" gazebo_resource_path="${prefix}:/" plugin_path="${prefix}/lib"/>
  </export>
</package>
