<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="environment" default="real" />
  <arg name="simulate_cams" default="true" />
  <include file="$(find subjugator_launch)/launch/subsystems/cameras/launch/right_camera.launch">
    <arg name="environment" value="$(arg environment)" />
  </include>
  <include file="$(find subjugator_launch)/launch/subsystems/cameras/launch/left_camera.launch">
    <arg name="environment" value="$(arg environment)" />
    <arg name="simulate_cams" value="$(arg simulate_cams)" />
  </include>
  <include file="$(find subjugator_launch)/launch/subsystems/cameras/launch/down_camera.launch">
    <arg name="environment" value="$(arg environment)" />
  </include>
</launch>
