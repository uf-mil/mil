<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <param name="/is_simulation" value="True" />

  <node pkg="mil_usb_to_can" type="driver.py" name="usb_to_can_driver">
    <rosparam>
    device_handles:
      "8": mil_usb_to_can.ExampleEchoDeviceHandle
    simulated_devices:
      "8": mil_usb_to_can.ExampleSimulatedEchoDevice
    </rosparam>
  </node>

  <test test-name="can_echo_test" pkg="mil_usb_to_can" type="echo_device_test.py" time-limit="5.0"/>
</launch>
