<?xml version="1.0"?>
<sdf version="1.5">
  <model name="dice5">
    <static>false</static>
    <link name="dice5">

    <!-- Change this to reflect real values -->

    <inertial>
      <mass>0.00001</mass>
      <inertia>
        <ixx>0</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0</iyy>
        <iyz>0</iyz>
        <izz>0</izz>
      </inertia>
    </inertial>
    <velocity_decay>
      <linear>2</linear>
      <angular>2</angular>
    </velocity_decay>


      <!--- Stolen again from the Torpedo for now. -->

      <collision name='dice5col'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>file://dice_2018/Dice5s.dae</uri>
          </mesh>
        </geometry>
      </collision>

      <sensor name="contact" type="contact">
        <contact>
          <collision>dice5col</collision>
        </contact>
        <update_rate>50</update_rate>
        <plugin name='gazebo_ros_bumper_controller' filename="libgazebo_ros_bumper.so">
          <alwaysOn>true</alwaysOn>
          <bumperTopicName>contact_bumper</bumperTopicName>
        </plugin>
      </sensor>

      <visual name="dice5vis">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>file://dice_2018/Dice5s.dae</uri>
          </mesh>
        </geometry>
  </model>
</sdf>
