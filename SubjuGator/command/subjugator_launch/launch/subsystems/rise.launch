<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="environment" default="real" />
  <node pkg="rise_6dof" type="rise_6dof.py" name="rise_6dof" output="screen" >
    <remap from="/current" to="odom"/>
    <remap from="/desired" to="trajectory"/>
    <remap from="/output" to="wrench"/>
    <rosparam unless="$(eval environment == 'gazebo')" file="$(find subjugator_launch)/config/rise_6dof.yaml"/>
    <rosparam if="$(eval environment == 'gazebo')" file="$(find subjugator_launch)/config/rise_6dof_gazebo.yaml"/>
  </node>
</launch>
