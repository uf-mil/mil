  <!-- Use YOLOv4 -->
  <arg name="network_param_file" default="$(find sub8_launch)/config/darknet_params/yolov4-tiny.yaml"/>
  <arg name="ros_param_file" default="$(find sub8_launch)/config/darknet_params/ros.yaml"/>
  <arg name="image" default="/camera/front/left/image_raw"/>
  <arg name="yolo_config_path"  default="$(find sub8_launch)/config/darknet_params"/>

  <!-- Main darknet_ros launch file-->
  <include file="$(find darknet_ros)/launch/darknet_ros.launch">
      <arg name="network_param_file" value="$(arg network_param_file)"/>
      <arg name="ros_param_file" value="$(arg ros_param_file)"/>
      <arg name="image" value="$(arg image)"/>
      <arg name="yolo_config_path" value="$(arg yolo_config_path)"/>
  </include>
</launch>